{
	"name": "@uniformdev/uniform-canvas-vnext-nextjs-playground",
	"version": "19.171.0",
	"private": true,
	"license": "Apache-2.0",
	"scripts": {
		"build": "run-s context:manifest build:next",
		"start": "next start --port 5952",
		"dev": "run-s context:manifest dev:next",
		"dev:next": "next dev --port 5952",
		"build:app": "run-s context:manifest build:next",
		"build:next": "next build",
		"context:manifest": "uniform context manifest download --output ./lib/context/manifest.json",
		"pull": "run-s pull:canvas pull:context",
		"push": "run-s push:context push:canvas push:project-map",
		"pull:canvas": "run-s pull:categories pull:components pull:patterns pull:compositions",
		"pull:compositions": "uniform presentation composition pull ./data/canvas/compositions --onlyCompositions",
		"pull:components": "uniform presentation component pull ./data/canvas/components",
		"pull:categories": "uniform presentation category pull ./data/canvas/categories",
		"pull:patterns": "uniform presentation pattern pull ./data/canvas/patterns",
		"push:project-map:definitions": "uniform project-map definition push ./data/project-map/definitions",
		"push:project-map:nodes": "uniform project-map node push ./data/project-map/nodes",
		"push:project-map": "run-s push:project-map:definitions push:project-map:nodes",
		"push:context:signals": "uniform context signal push ./data/context/signals",
		"push:context:enrichments": "uniform context enrichment push ./data/context/enrichments",
		"push:context:aggregates": "uniform context aggregate push ./data/context/aggregates",
		"push:context:quirks": "uniform context quirk push ./data/context/quirks",
		"push:context:tests": "uniform context test push ./data/context/tests",
		"push:context": "run-s push:context:signals push:context:enrichments push:context:quirks push:context:tests push:context:aggregates",
		"pull:context:signals": "uniform context signal pull ./data/context/signals",
		"pull:context:enrichments": "uniform context enrichment pull ./data/context/enrichments",
		"pull:context:aggregates": "uniform context aggregate pull ./data/context/aggregates",
		"pull:context:quirks": "uniform context quirk pull ./data/context/quirks",
		"pull:context:tests": "uniform context test pull ./data/context/tests",
		"pull:context": "run-s pull:context:signals pull:context:enrichments pull:context:quirks pull:context:tests pull:context:aggregates",
		"push:compositions": "uniform presentation composition push ./data/canvas/compositions --onlyCompositions",
		"push:compositions:publish": "uniform canvas composition publish --all",
		"push:components": "uniform presentation component push ./data/canvas/components",
		"push:categories": "uniform presentation category push ./data/canvas/categories",
		"push:patterns": "uniform presentation pattern push ./data/canvas/patterns",
		"push:patterns:publish": "uniform presentation pattern publish --all",
		"push:patterns:unpublish": "uniform presentation pattern unpublish --all",
		"push:canvas": "run-s push:categories push:components push:compositions push:patterns push:compositions:publish",
		"package": "run-s package:canvas package:context",
		"package:canvas": "run-s package:categories package:components package:compositions ",
		"package:compositions": "uniform presentation composition pull ./data/uniformconf.yaml",
		"package:components": "uniform presentation component pull ./data/uniformconf.yaml",
		"package:categories": "uniform presentation category pull ./data/uniformconf.yaml",
		"package:context:signals": "uniform context signal pull ./data/uniformconf.yaml",
		"package:context:enrichments": "uniform context enrichment pull ./data/uniformconf.yaml",
		"package:context:aggregates": "uniform context aggregate pull ./data/uniformconf.yaml",
		"package:context:quirks": "uniform context quirk pull ./data/uniformconf.yaml",
		"package:context:tests": "uniform context test pull ./data/uniformconf.yaml",
		"package:context": "run-s package:context:signals package:context:enrichments package:context:quirks package:context:tests package:context:aggregates",
		"project:pull": "uniform sync pull",
		"project:push": "uniform sync push",
		"project:pull:prod-config": "uniform sync pull --config ./uniform.prod.config.ts",
		"project:push:prod-config": "uniform sync push --config ./uniform.prod.config.ts",
		"cli": "uniform",
		"start:stackblitz": "npm run dev:next"
	},
	"dependencies": {
		"@analytics/google-analytics": "1.0.7",
		"@react-hook/window-scroll": "1.3.0",
		"@uniformdev/canvas": "^19.170.1",
		"@uniformdev/canvas-next": "^19.170.1",
		"@uniformdev/canvas-react": "^19.170.1",
		"@uniformdev/context": "^19.170.1",
		"@uniformdev/context-devtools": "^19.170.1",
		"@uniformdev/context-gtag": "^19.170.1",
		"@uniformdev/context-next": "^19.170.1",
		"@uniformdev/context-react": "^19.170.1",
		"@uniformdev/context-edge": "^19.170.1",
		"@uniformdev/context-edge-vercel": "^19.170.1",
		"analytics": "0.8.9",
		"cookie": "0.6.0",
		"cross-env": "7.0.3",
		"next": "14.2.5",
		"nookies": "2.5.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"styled-components": "5.3.11"
	},
	"devDependencies": {
		"@emotion/react": "11.11.3",
		"@netlify/functions": "2.5.1",
		"@netlify/plugin-nextjs": "4.41.3",
		"@next/bundle-analyzer": "14.0.4",
		"@types/cookie": "0.6.0",
		"@types/node": "20.10.6",
		"@types/react": "18.2.40",
		"@types/react-dom": "18.2.17",
		"@types/styled-components": "5.1.34",
		"@uniformdev/cli": "^19.170.1",
		"autoprefixer": "10.4.16",
		"dotenv": "16.3.1",
		"node-fetch": "2.7.0",
		"npm-run-all2": "6.2.2",
		"postcss": "8.4.38",
		"sass": "1.59.2",
		"tailwindcss": "3.4.0",
		"ts-loader": "9.4.3",
		"typescript": "5.3.3"
	},
	"stackblitz": {
		"startCommand": "npm run start:stackblitz",
		"compileTrigger": "save"
	}
}
